{
  "name": "goobs-cache",
  "version": "1.7.2",
  "description": "A versatile caching and state management solution for JavaScript applications. Offers serverless, session, and cookie-based caching with support for encryption, compression, and auto-optimization. Provides a Jotai-like API for seamless integration. Handles complex data structures and enables cross-environment persistence.",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "build": "yarn pretty && yarn lint && yarn typecheck",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "pretty": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepublishOnly": "yarn build",
    "publish-package": "npm publish",
    "test": "jest"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240821.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@types/lz4js": "^0.2.1",
    "@types/react": "^18.3.4",
    "goobs-encryption": "^0.1.2",
    "goobs-testing": "^0.2.2",
    "jotai": "^2.9.3",
    "lru-cache": "^10.3.0",
    "lz4js": "^0.2.0",
    "next": "^14.2.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "winston": "^3.14.2"
  },
  "files": [
    "src"
  ],
  "keywords": [
