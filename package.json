{
  "name": "goobs-cache",
  "version": "1.4.0",
  "description": "A comprehensive data management solution for JavaScript applications, offering versatile caching (including Redis-like functionality), state management (similar to Jotai), and storage options (cookies, session storage, server-side). Features encryption, compression, auto-optimization, and supports various data structures.",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "build": "yarn lint && yarn typecheck && yarn pretty",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "pretty": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepublishOnly": "yarn build",
    "publish-package": "npm publish",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.2.3",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@types/lz4js": "^0.2.1",
    "@types/react": "^18.3.3",
    "lru-cache": "^10.3.0",
    "lz4js": "^0.2.0",
    "react": "^18.3.1"
  },
  "files": [
    "src"
  ],
  "keywords": [
    "cache",
    "encrypted",
    "compressed",
    "storage",
    "state-management",
    "jotai-alternative",
    "atom",
    "server-side",
    "client-side",
    "cookies",
    "session-storage",
    "encryption",
    "compression",
    "lru",
    "key-value",
    "data-structures",
    "javascript",
    "typescript",
    "context",
    "persistence",
    "aes-gcm",
    "key-rotation",
    "react",
    "cross-environment",
    "async-context",
    "zlib",
    "lz4"
  ],
  "author": "Matthew Goluba",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/goobz22/goobs-cache"
  }
}
